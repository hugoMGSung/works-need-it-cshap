<Window x:Class="wp05_bikeShop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wp05_bikeShop"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="480" Width="600" MaxWidth="600" MaxHeight="480" ResizeMode="NoResize" 
        WindowStartupLocation="CenterScreen">
    <Grid>
        <!-- Frame; 자식페이지를 할당할 수 있는 컨트롤 -->
        <Frame x:Name="MainFrame" Background="SeaShell" Source="/MenuPage.xaml"
               NavigationUIVisibility="Visible" />
    </Grid>
</Window>
